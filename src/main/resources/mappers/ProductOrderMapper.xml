<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.keyrhyme.fromearth.mapper.ProductOrderMapper">
    <select id="selectOrderById" resultType="com.keyrhyme.fromearth.model.Order">
        SELECT p.userid, p.name, p.pno, p.post_code, p.address1, p.address2, p.memo
        FROM product_order p
        WHERE id = 1;
    </select>

</mapper>